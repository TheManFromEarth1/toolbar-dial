{
  "name": "toolbar-dial-firefox",
  "author": {
    "name": "Lucas Everett",
    "email": "lucas@lucaseverett.dev",
    "url": "https://lucaseverett.dev"
  },
  "verion": "2.0.1",
  "description": "Toolbar Dial is a browser extension that replaces the New Tab page with a colorful grid of bookmarks and folders.",
  "homepage": "https://toolbardial.com",
  "keywords": [
    "firefox",
    "speed dial",
    "toolbar dial"
  ],
  "license": "MIT",
  "dependencies": {
    "@jaames/iro": "^5.1.8",
    "emotion": "^10.0.27",
    "focus-visible": "^5.1.0",
    "fontawesome": "^5.6.3",
    "normalize.css": "^8.0.1",
    "random-material-color": "^1.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sortablejs": "^1.10.2"
  },
  "scripts": {
    "start": "web-ext run",
    "build": "rimraf dist && parcel build ./src/*.html --no-cache --public-url ./"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.9.6",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "rimraf": "^3.0.2",
    "web-ext": "^4.2.0"
  },
  "browserslist": [
    "last 2 Firefox versions"
  ],
  "staticFiles": {
    "staticPath": [
      "src/wallpapers/wallpapers",
      "src/wallpapers/thumbs"
    ],
    "watcherGlob": "**"
  }
}
